    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="data\" />
    <None Remove="logic\" />
    <None Remove="ui\" />
    <None Remove="data\repo\" />
    <None Remove="Newtonsoft.Json" />
